{
    "root": true,
    "parser": "@babel/eslint-parser",
    "env": {
        "node": true
    },
    "extends": [
        "plugin:vue/essential",
        "eslint:recommended",
        "@vue/airbnb"
    ],
    "rules": {
        "max-len": "off"
    },
    "parserOptions": {
        "parser": "@babel/eslint-parser",
        "sourceType": "module",
        "babelOptions": {
            "presets": ["@babel/preset-env"]
          }
    }
}